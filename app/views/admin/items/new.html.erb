<h1>新規登録</h1>
<table>
  <%= form_with model: @item, url: admin_items_path, method: :post, local:true do |f| %>
    <tr>
      <td>新規画像</td>
      <td><%= f.file_field :image %></td>
    </tr>
    <tr>
      <td>商品名</td>
      <td><%= f.text_field :name %></td>
    </tr>
    <tr>
      <td>商品説明</td>
      <td><%= f.text_area :description %></td>
    </tr>
    <tr>
      <td>ジャンル</td>
      <td></td>
    </tr>
    <tr>
      <td>税込価格</td>
      <td><%= f.text_field :no_tax %></td>
    </tr>
      <td>販売ステータス</td>
      <td></td>
    </tr>
    <tr>
      <td><%= f.submit '新規登録' %></td>
    </tr>
  <% end %>
</table>