<div>
  <h2>配送先編集</h2>
    <%= form_with(model: @address, url:address_path, local: true ,method: :patch) do |f| %>
      <table>
        <tr>
          <td>郵便番号(ハイフンなし)</td>
          <td><%= f.text_field :postcode, aoutfocus: true %></td>
        </tr>
        <tr>
          <td>住所</td>
          <td><%= f.text_field :address %></td>
        </tr>
        <tr>
          <td>宛名</td>
          <td><%= f.text_field :name %></td>
        </tr>
      </table>

      <%= f.submit '変更内容を保存', class:"btn btn-primary btn-lg pull-right" %>
      <% end %>
  </div>